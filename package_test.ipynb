packets = [2, 3, 6, 1, 1, 1, 2, 3, 4, 5, 1, 2, 4, 2, 1, 3, 10, 8]

count = 0

for i in range(len(packets)):
    if i == 0:
        # First packet, do nothing
        continue
    current = packets[i]
    previous = packets[i-1]

    if current <= previous:
        # Reset or repeat â†’ add previous value to count
        count += previous

count += packets[-1] -1        

print("Count:", count)
